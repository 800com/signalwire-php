{
  "name": "signalwire/signalwire",
  "description": "Client library for connecting to SignalWire.",
  "type": "library",
  "keywords": [
    "voip",
    "sms",
    "mms",
    "voice",
    "voicemail",
    "video",
    "fax",
    "api",
    "ivr",
    "iot",
    "freeswitch",
    "signalwire",
    "relay",
    "laml"
  ],
  "homepage": "https://github.com/signalwire/signalwire-php",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SignalWire\\": "src/"
    },
    "files": [ "src/functions.php", "src/Version.php" ]
  },
  "authors": [
    {
      "name": "SignalWire Team",
      "email": "open.source@signalwire.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "twilio/sdk": "6.37.1",
    "ratchet/pawl": "^0.4.1",
    "monolog/monolog": "^3.0",
    "ramsey/uuid": "^3.8 || ^4.0",
    "recoil/react": "^1.0",
    "guzzlehttp/guzzle": ">=6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "php-vcr/php-vcr": "^1.6.0",
    "symfony/var-dumper": "^6.2"
  }
}
